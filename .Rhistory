# Identify Nest, Perch, Flight, Cruise, and Roost Behavior ---------------------
# Isolate periods birds were "Territorial" (e.g., they stayed near their nest,
# it was the breeding season, etc.)
# remove non-terrotorial birds
baea_terr <- baea %>% filter(!id %in% c("Cherryfield", "Davis", "Madagascal",
"Webb"))
# filter to only summer months (April 1- August 31)
baea_terr %>%
group_by(id) %>%
summarize(start_date = first(date),
last_date = last(date)) %>%
ungroup(.) %>%
mutate(
date_period = as.period(interval(start_date, last_date), unit = "months"))
library(zoo)
baea_terr2 <- baea_terr %>%
group_by(id, date) %>%
summarize(nest_dist_dymean = mean(nest_dist)) %>%
mutate(nest_dist_dymean3 = rollmean(nest_dist_dymean, 3, fill=NA, na.pad=TRUE,
align="left")) %>%
ungroup()
by_colors <- CreateColorsByAny(by="id", df=baea, output=TRUE)
for (i in unique(baea_terr2$id)){
print(i)
baea_terr_i <- baea_terr2 %>% filter(id == i)
min_date <- as.POSIXct(min(baea_terr_i$date))
max_date <- min(which(is.na(baea_terr_i$nest_dist_dymean)))
max_date <- as.POSIXct(baea_terr_i$date[max_date])
g <- ggplot(data = baea_terr_i) +
geom_line(aes(x=as.POSIXct(date), y=nest_dist_dymean3/1000, color="blue"),
size=.5, show.legend = FALSE) +
scale_y_continuous(expand = c(0, 0, 0.05, 0)) +
#    scale_y_continuous(limits = c(0, 10), expand = c(0,0)) +
scale_x_datetime(breaks=scales::date_breaks("1 month"), labels =
scales::date_format("%b %Y"), limits = c(min_date, max_date)) +
theme(axis.text.x = element_text(angle = 45, hjust = 1, colour = "black")) +
labs(x = "Date", y = "Running Mean 3- Day Nest Distance (km)", title = i) +
theme(plot.title = element_text(hjust = 0.5)) +
theme(plot.title=element_text(size=18, face="bold", vjust=1.5)) +
theme(axis.title=element_text(size=14, face="bold")) +
theme(axis.text=element_text(colour="black")) +
theme(axis.text.x=element_text(size=12, angle=50, vjust=1)) +
theme(axis.text.y=element_text(size=12, vjust=0.5))
plot(g)
}
